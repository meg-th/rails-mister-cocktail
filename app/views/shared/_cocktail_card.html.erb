<% @cocktails.each do |cocktail|%>
  <li>
    <% if cocktail.photo.attached? %>
    <%= cl_image_tag cocktail.photo.key, height: 300, width: 400, crop: :fill %> 
    <h3><%=link_to cocktail.name, cocktail_path(cocktail.id)%></h3>
    <% else %>
    <h3><%=link_to cocktail.name, cocktail_path(cocktail.id)%></h3>
    <% end %>
  </li>
<%end%>